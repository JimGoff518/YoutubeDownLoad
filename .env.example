# YouTube Data API Key
# Get your API key from: https://console.cloud.google.com/apis/credentials
YOUTUBE_API_KEY=your_api_key_here

# Processing Configuration
MAX_CONCURRENT_VIDEOS=5

# Output Configuration
OUTPUT_DIR=./output

# Audio Transcription Fallback (when YouTube transcripts unavailable)
# Set to true to enable downloading audio and using Whisper for transcription
ENABLE_AUDIO_FALLBACK=false

# Whisper model size: tiny, base, small, medium, large
# base = recommended (good balance of speed and accuracy)
# tiny = fastest but less accurate
# large = most accurate but slower and requires more RAM
WHISPER_MODEL=base

# Cleanup downloaded audio files after transcription
CLEANUP_AUDIO=true
